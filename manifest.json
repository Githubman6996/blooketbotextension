{
  "name": "Blooket Bot",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Made by Ducklife3141 | Cryptodude3",
  "homepage_url": "https://blooketbot.glitch.me/",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
	      "*://fb.blooket.com/debug-test"
      ],
      "js": [
        "blooketbot.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "declarativeNetRequest",
    "browsingData",
    "tabs"
  ],
  "host_permissions": [
    "*://fb.blooket.com/debug-test"
  ]
}
